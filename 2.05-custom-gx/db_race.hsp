		if ( dbmode == DBMODE_REF_PLAYABLE ) {
			return 0
		}
		if ( dbmode == DBMODE_SET ) {
			cdatan(CDATAN_RACE, rc) = "vehicle"
			cdata(CDATA_RACE_ID, rc) = DB_RACE_BEHICLE
			cdata(CDATA_MELEE_STYLE, rc) = 10
