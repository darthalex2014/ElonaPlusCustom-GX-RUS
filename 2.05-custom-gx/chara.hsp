	s = lang("ЉF–і", "Нет "), lang("ЌЕЌ‚", "Лучший "), lang("—ќ‘z“I", "Отличный "), lang("Ќ‚‚ў", "Хороший "), lang("‚в‚вЌ‚‚ў", "Неплохой "), lang("•Ѓ’К", "Нормальный "), lang("Џ­‚µ", "Маленький "), lang("”чЃX", "Небольшой ")
	locvar_cm_refer_scale_attr = 0, 3, 5, 7, 9, 11, 13
	locvar_cm_refer_scale_spd = 0, 25, 50, 75, 100, 125, 150
	locvar_cm_refer_scale_spdadd = 0, 3, 6, 9, 12, 15, 18
	locvar_cm_refer_scale_lifemana = 0, 33, 66, 100, 133, 166, 200
	locvar_cm_refer_rows = 3
	if ( val == 0 ) { ; Races
		locvar_cm_refer_rows = 4
	}
	repeat locvar_cm_refer_rows
		if ( val == 1 & cnt > 3 ) { ; Classes
			break
		}
		cnt2 = cnt
		repeat 3
			if ( cnt2 == 2 & cnt == 2 ) {
				r = SKILL_ATTR_SPD
				if ( val == 0 ) { ; Races
					dup locvar_cm_refer_scale, locvar_cm_refer_scale_spd
				}
				else { ; Classes
					dup locvar_cm_refer_scale, locvar_cm_refer_scale_spdadd
				}
			}
			else {
				if ( cnt2 == 3 ) {
					dup locvar_cm_refer_scale, locvar_cm_refer_scale_lifemana
					if ( cnt == 0 ) {
						r = SKILL_ATTR_LIFE
					}
					if ( cnt == 1 ) {
						r = SKILL_ATTR_MANA
					}
					if ( cnt >= 2 ) {
						break
					}
				}
				else {
					r = cnt2 * 3 + cnt + STARTING_SKILL_ATTR
					dup locvar_cm_refer_scale, locvar_cm_refer_scale_attr
				}
			}
			p = 0
			repeat 1
				if ( sorg(r, CHARA_PLAYER) > locvar_cm_refer_scale(6) ) {
					p = 1
					color 0, 0, 200
					break
				}
				if ( sorg(r, CHARA_PLAYER) > locvar_cm_refer_scale(5) ) {
					p = 2
					color 0, 0, 200
					break
				}
				if ( sorg(r, CHARA_PLAYER) > locvar_cm_refer_scale(4) ) {
					p = 3
					color 0, 0, 150
					break
				}
				if ( sorg(r, CHARA_PLAYER) > locvar_cm_refer_scale(3) ) {
					p = 4
					color 0, 0, 150
					break
				}
				if ( sorg(r, CHARA_PLAYER) > locvar_cm_refer_scale(2) ) {
					p = 5
					color 0, 0, 0
					break
				}
				if ( sorg(r, CHARA_PLAYER) > locvar_cm_refer_scale(1) ) {
					p = 6
					color 150, 0, 0
					break
				}
				if ( sorg(r, CHARA_PLAYER) > locvar_cm_refer_scale(0) ) {
					p = 7
					color 200, 0, 0
					break
				}
				color 120, 120, 120
			loop
			pos cnt * 150 + tx + 13, ty + 7
			gmode 2, inf_tiles, inf_tiles
			locvar_cm_refer_icon = (cnt2 * 3 + cnt)
			if ( r == SKILL_ATTR_LIFE ) {
				locvar_cm_refer_icon = 10 ; Shield icon
			}
			if ( r == SKILL_ATTR_MANA ) {
				locvar_cm_refer_icon = 15 ; Hat icon
			}
			if ( r == SKILL_ATTR_SPD ) {
				locvar_cm_refer_icon = 11 ; Shoe icon
			}
			grotate 1, locvar_cm_refer_icon * inf_tiles, 672, 0, inf_tiles, inf_tiles
			pos cnt * 150 + tx + 32, ty
			if ( cfg_cmPreciseStats ) {
				if ( val == 0 ) { ; Races
					mes strmid(skillname(r), 0, 4 - (jp == 0)) + ": " + sorg(r, CHARA_PLAYER)
				}
				else { ; Classes
					mes strmid(skillname(r), 0, 4 - (jp == 0)) + ": " + cnvfix(sorg(r, CHARA_PLAYER))
				}
			}
			else {
				mes strmid(skillname(r), 0, 4 - (jp == 0)) + ": " + s(p)
			}
		loop
		ty += 16
	loop
	ty = wy + 250
	if ( val == 0 ) { ; Races
		ty += 16 ; Extra row for life/mana
		display_topic lang("Љl“ѕ•”€К", "Части Тела "), tx, ty
		ty += 34
		font lang(cfg_font1, cfg_font2), 14 - en * 2, 0
		pos tx + 32, ty
		mes bodyparts2string()
		ty += 18
	}
	display_topic lang("Љl“ѕ‹Z”\", "Обученные навыки "), tx, ty
	ty += 34
	font lang(cfg_font1, cfg_font2), 14 - en * 2, 0
	color 0, 0, 0
	r = 0
	s = lang("•ђЉн‚Мђк–е  ", "Знаток в ")
	repeat STARTING_SKILL_NORMAL - STARTING_SKILL_WEAPON, STARTING_SKILL_WEAPON
		if ( sorg(cnt, CHARA_PLAYER) != 0 ) {
			if ( r != 0 ) {
				s += ","
			}
			s += skillname(cnt)
			r++
		}
	loop
	if ( r != 0 ) {
		pos tx + 13, ty + 6
		gmode 2, inf_tiles, inf_tiles
		grotate 1, 0, 672, 0, inf_tiles, inf_tiles
		pos tx + 32, ty
		mes s
		ty += 14
	}
	repeat STARTING_SKILL_SPACT - STARTING_SKILL_NORMAL, STARTING_SKILL_NORMAL
		if ( sorg(cnt, CHARA_PLAYER) != 0 ) {
			s = skillname(cnt)
			if ( jp ) {
				lenfix s, 12
			}
			else {
				lenfix s, 16
			}
			pos tx + 13, ty + 6
			gmode 2, inf_tiles, inf_tiles
			grotate 1, (sdataref(SKILL_DATAREF_USE, cnt) - 10) * inf_tiles, 672, 0, inf_tiles, inf_tiles
			s(1) = skilldesc(cnt)
			if ( en ) {
				if ( strlen(s(1)) > 45 ) {
					s(1) = strmid(s(1), 0, 42) + "..."
				}
			}
			pos tx + 32, ty
			mes s + s(1)
			ty += 14
		}
	loop
	return

#defcfunc bodyparts2string
	dim locvar_bp2s_parts, MAX_EQUIP_SLOT
	repeat MAX_EQUIP_SLOT_TOTAL, CDATA_STARTING_EQUIP_SLOTS
		locvar_bp2s_part_type = cdata(cnt, CHARA_PLAYER) / 10000
		if ( locvar_bp2s_part_type == EQUIP_SLOT_NONE | locvar_bp2s_part_type == EQUIP_SLOT_SHOOT | locvar_bp2s_part_type == EQUIP_SLOT_AMMO ) {
			break
		}
		locvar_bp2s_parts(locvar_bp2s_part_type) += 1
	loop
	locvar_bp2s_result = ""
	repeat length(locvar_bp2s_parts)
		locvar_bp2s_count = locvar_bp2s_parts(cnt)
		if ( locvar_bp2s_count == 0 ) {
			continue
		}
		locvar_bp2s_result += bodyn(cnt)
		if ( locvar_bp2s_count != 1 ) {
			locvar_bp2s_result += "x" + locvar_bp2s_count
		}
		locvar_bp2s_result += " "
	loop
	return locvar_bp2s_result
